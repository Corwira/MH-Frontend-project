  body
    
    //top navigation
    .nav
        .nav__logo
            img(src='img/logo.png' alt='logo')
        ul.nav__list
            li.nav__list__item 
                a(href='#') home
            li.nav__list__item 
                a(href='#') about
            li.nav__list__item 
                a(href='#') pricing
            li.nav__list__item 
                a(href='#') domain
            li.nav__list__item
                a(href='#') hosting
        .line
        .nav__info
            p.nav__info__text Do you need help? Just Email, Live chat or Call us
            .nav__info__contacts
                button Live Chat
                .line-sep
                p 6284-6534
              
    .header
       h1.header__leading.animated.wow.fadeInDown Just a Proffesional Web Hosting Services
