       img.animated.wow.fadeInDown(src='img/central-shape.png'  alt='shape')
       .header__input.animated.wow.fadeInDown
           input(type='text' placeholder='your domain name...')
           select
               option .com
               option .ru
               option .org
           button Check Availability
    
